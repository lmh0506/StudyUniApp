.movie-score-wrapper {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
}
.movie-score-wrapper .bg-star {
    width: 50px;
    height: 10px;
    margin-top: 6rpx;
    background: url(data:image/png;base64,);
    background-size: 10px 10px;
    position: relative;
}
.movie-score-wrapper .bg-star .active-star {
      position: absolute;
      top: 0px;
      left: 0px;
      height: 100%;
      background: url(data:image/png;base64,);
      background-size: 10px 10px;
}
.movie-score-wrapper .star {
    width: 20rpx;
    height: 20rpx;
    margin-top: 6rpx;
}
.movie-score-wrapper .movie-score {
    font-size: 12px;
    color: gray;
    margin-left: 8rpx;
}

